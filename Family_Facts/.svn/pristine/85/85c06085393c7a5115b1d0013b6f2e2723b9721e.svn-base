 div.tctable.active
 {
	
	display: block;
	background-color:#f1f7f9;
	position:relative;
	width:75%;
	float:left;
 }
 div.tctable
 {
	 background-color:#f1f7f9;
	 position:relative;
	 width:75%;
	 float:left;
	display: none;
 }
 
 .testsenarios
	 {
		display: none;
	 }

	 
    
 .testsenarios.black_overlay
	{
        display: none;
        position: absolute;
        top: 0%;
        left: 0%;
        width: 100%;
        height: 100%;
        background-color: red;
        z-index:4;
        -moz-opacity: 0.8;
        opacity:1.0;
        filter: alpha(opacity=100);
    }
 .testsenarios.white_content 
	{
        display: none;
        position: absolute;
        top: 10%;
        left: 10%;
        width: 75%;
        height: 70%;
        padding: 6px;
        border: 5px groove lightgreen;
        background-color: white;
        z-index:6;
        overflow: hidden;
    }

.close
{
	display: none;
        position: absolute;
        top: 0%;
        left: 95%;
        width: 5%;
        height: 5%;
        //background-color: black;
        z-index:5;
        -moz-opacity: 0.8;
        opacity:.80;
        filter: alpha(opacity=80);
}

.movements
{
    width: 100px;
    height: 100px;
    background: red;
    position: relative;
    -webkit-animation: mymove 0.5s;  /* Safari and Chrome */
    -webkit-animation-iteration-count: 1;  /* Safari and Chrome */
    -webkit-animation-fill-mode: forwards;  /* Safari and Chrome */
    animation: mymove 0.5s;
    animation-iteration-count: 1;
    animation-fill-mode: forwards;
}

/* Safari and Chrome */
@-webkit-keyframes mymove {
    from {left: 0px;}
    to {right: 130px;}
}

@keyframes mymove {
    from {left: 0px;}
    to {right: 130px;}
}
