 /* Light Box Style */
	
	#fade{
		background-color: #000;
		display: none;
		height: 100%;
		left: 0;
		opacity: 0.7;
		position: fixed;
		top: 0;
		z-index:1;
		width: 100%;
	}
	
 .testsenarios.white_content 
	{
        
		display: none;
        position: absolute;
        top: 12%;
		left: 10%;
        width: 85%;
        height: 80%;
		padding: 5px;
        background-color: white;
        z-index:2;
        overflow: hidden;
    }

.close
{
		display: none;
        position: absolute;
        top: 10px;
        left: 95%;
        width: 5%;
        height: 5%;
        background-color: white;
        z-index:4;
        -moz-opacity: 0.8;
        opacity:.80;
        filter: alpha(opacity=80);
}

/* Moments for TestSenarios Wizard */

.movements
{
    width: 100px;
    height: 100px;
    background: red;
    position: relative;
    -webkit-animation: mymove 0.5s;  /* Safari and Chrome */
    -webkit-animation-iteration-count: 1;  /* Safari and Chrome */
    -webkit-animation-fill-mode: forwards;  /* Safari and Chrome */
    animation: mymove 0.5s;
    animation-iteration-count: 1;
    animation-fill-mode: forwards;
}

/* Safari and Chrome */
@-webkit-keyframes mymove {
    from {left: 0px;}
    to {right: 130px;}
}

@keyframes mymove {
    from {left: 0px;}
    to {right: 130px;}
}


